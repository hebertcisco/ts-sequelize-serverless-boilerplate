{
  "name": "ts-sequelize-serverless-boilerplate",
  "version": "0.0.1",
  "description": "Î» TypeScript & Sequelize Serverless Boilerplate",
  "main": "event.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "test:cov": "npm test -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm test -- --config jest-e2e.json",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "serverless:dev": "serverless offline start",
    "serverless:predev": "npm run build",
    "serverless:deploy": "serverless deploy --verbose"
  },
  "engines": {
    "node": ">=16.16.0",
    "npm": ">=8.11.0"
  },
  "dependencies": {
    "@middy/core": "^4.1.0",
    "@middy/http-json-body-parser": "^4.1.0",
    "axios": "^1.2.2",
    "dotenv": "^16.0.3",
    "jest-cucumber": "^3.0.1",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/jest": "29.4.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.1",
    "esbuild": "^0.16.17",
    "eslint": "^8.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.2",
    "serverless": "^3.26.0",
    "serverless-esbuild": "^1.37.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",
    "ts-jest": "29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "license": "MIT"
}